<?xml version="1.0" encoding="utf-8"?>
<building>
	<modifiers>
		<modifier visible="0">
			<effects>
				<productionCost base="48"/>
				<oreCost base="80"/>
				<populationRequired base="1"/>
				<slotsRequired base="1"/>
				<energyUpkeep add="3"/>
			</effects>
		</modifier>
		<modifier>
			<effects>
				<growth add="1"/>
				<ore add="2"/>
				<production add="6"/>
				<research add="2"/>
			</effects>
		</modifier>
	</modifiers>
	<productionModifiers>
		<modifier>
			<effects>
				
			</effects>
		</modifier>
	</productionModifiers>
	<strategyModifiers>
		<modifier>
			<effects>
<increaseConstructionScore base="1.2"/>
<increaseAircraftScore base="1.2"/>
			</effects>
		</modifier>
	</strategyModifiers>

	<actions>
		<acquireTile>
			<modifiers>
				<modifier visible="0">
					<effects>
						<influenceCost add="5"/>
						<oreCost add="5"/>
						<productionCost add="12"/>
					</effects>
				</modifier>
			</modifiers>
		</acquireTile>
		<clearTile>
			<modifiers>
				<modifier visible="0">
					<effects>
						<productionCost add="12"/>
					</effects>
				</modifier>
			</modifiers>
		</clearTile>
		<constructBuilding building="Eldar/Food"/>
		<constructBuilding building="Eldar/Ore"/>
		<constructBuilding building="Eldar/Energy"/>

		<!-- worthy research -->
		<constructBuilding building="Eldar/Research">
			<modifiers>
                <modifier visible="0">
                    <conditions>
                        <player><aiControlled/></player>
                    </conditions>
                    <effects>
                        <cooldown set="999"/>
                    </effects>
                </modifier>
            </modifiers>
        </constructBuilding>

		<constructBuilding building="Eldar/Influence"/>
		<constructBuilding building="Eldar/Loyalty" requiredUpgrade="Eldar/Loyalty"/>
		<constructBuilding building="Eldar/Housing" requiredUpgrade="Eldar/Housing"/>
		<constructBuilding building="Eldar/Infantry"/>

		<!-- worthy heroes -->
		<constructBuilding building="Eldar/Heroes" requiredUpgrade="Eldar/Heroes">
			<modifiers>
                <modifier visible="0">
                    <conditions>
                        <player><aiControlled/></player>
                    </conditions>
                    <effects>
                        <cooldown set="999"/>
                    </effects>
                </modifier>
            </modifiers>
        </constructBuilding>

		<constructBuilding building="Eldar/Vehicles" requiredUpgrade="Eldar/Vehicles"/>
		<constructBuilding building="Eldar/Aircraft" requiredUpgrade="Eldar/Aircraft"/>

		<!-- worthy construction -->
		<constructBuilding building="Eldar/Construction" requiredUpgrade="Eldar/Construction">
			<modifiers>
                <modifier visible="0">
                    <conditions>
                        <player><aiControlled/></player>
                    </conditions>
                    <effects>
                        <cooldown set="999"/>
                    </effects>
                </modifier>
            </modifiers>
        </constructBuilding>

	</actions>
	<traits>
		<trait name="Eldar/ConstructionBuildingBonus" requiredUpgrade="Eldar/ConstructionBuildingBonus"/>
	</traits>
</building>
